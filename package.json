{
  "name": "markdownlint",
  "version": "0.0.1",
  "description": "A Node.js style checker and lint tool for Markdown files.",
  "main": "lib/markdownlint.js",
  "author": "David Anson (http://dlaa.me/)",
  "license": "MIT",
  "homepage": "https://github.com/DavidAnson/markdownlint",
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidAnson/markdownlint.git"
  },
  "bugs": {
    "url": "https://github.com/DavidAnson/markdownlint/issues"
  },
  "scripts": {
    "test": "nodeunit",
    "test-cover": "istanbul cover node_modules/nodeunit/bin/nodeunit",
    "debug": "node debug node_modules/nodeunit/bin/nodeunit",
    "lint": "eslint lib test & eslint --rule \"no-console: 0\" example",
    "example": "cd example & node standalone.js & gulp markdownlint"
  },
  "dependencies": {
    "markdown-it": "^3.0.7"
  },
  "devDependencies": {
    "eslint": "^0.15.0",
    "istanbul": "^0.3.7",
    "nodeunit": "^0.9.0",
    "q": "^1.2.0"
  },
  "keywords": [
    "markdown",
    "lint",
    "md"
  ]
}
